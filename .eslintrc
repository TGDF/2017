{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/no-danger": [0],
    "no-underscore-dangle": [2, { "allow": [__PRELOADED_STATE__] }],
  },
  "env" :{
    "browser": true,
  }
}
